    class Solution {
    public int countNegatives(int[][] grid) {
        int l=grid.length;
        int ll=grid[0].length;
        int c=0;
        for(int i=0;i<l;i++){
            for(int j=ll-1;j>=0;j--){
                if(grid[i][j]>=0) break;
                c++;
            }
        }
        return c;
    }
}