class Solution {
    public boolean checkOnesSegment(String s) {
        int l=s.length();
        boolean fl=true;
        int i=0;
        while(i<l && s.charAt(i)=='1'){
            i++;
        }
        while(i<l){
            if(s.charAt(i)=='1'){
                return false;
            }
            i++;
        }
        return true;
    }
}