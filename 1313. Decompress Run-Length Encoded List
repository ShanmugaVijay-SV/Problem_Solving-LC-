class Solution {
    public int[] decompressRLElist(int[] nums) {
        List<Integer> lst=new ArrayList<>();
        int l=nums.length;
        for(int i=0;i<l;i+=2){
            for(int j=0;j<nums[i];j++){
                lst.add(nums[i+1]);
            }
        }
        int ll=lst.size();
        int arr[]=new int[ll];
        for(int i=0;i<ll;i++){
            arr[i]=lst.get(i);
        }
        return arr;
    }
}