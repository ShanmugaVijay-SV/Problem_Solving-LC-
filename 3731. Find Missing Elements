class Solution {
    public List<Integer> findMissingElements(int[] nums) {
        List<Integer> lst=new ArrayList<>();
        int l=nums.length;
        int mx=nums[0];
        int mn=nums[0];
        for(int i=0;i<l;i++){
            if(nums[i]>mx) mx=nums[i];
            if(nums[i]<mn) mn=nums[i];
        }
        int arr[]=new int[mx+1];
        for(int i=0;i<l;i++){
            arr[nums[i]]++;
        }
        for(int i=mn;i<=mx;i++){
            if(arr[i]==0){
                lst.add(i);
            }
        }
        return lst;
    }
}