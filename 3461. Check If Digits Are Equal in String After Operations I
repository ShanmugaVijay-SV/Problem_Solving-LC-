class Solution(object):
    def hasSameDigits(self, s):
        """
        :type s: str
        :rtype: bool
        """
        while len(s)!=2:
            aux=""
            for j in range(1,len(s)):
                n=(int(s[j])+int(s[j-1]))%10
                aux=aux+str(n)
            s=aux
        return len(s) == 2 and s[0]==s[1]
                
        