class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        int l=letters.length;
        if(letters[l-1]<=target) return letters[0];
        for(int i=0;i<l;i++){
            if(letters[i]>target){
                return letters[i];
                
            }
        }
        return 'a';
    }
}