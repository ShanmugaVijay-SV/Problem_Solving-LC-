class Solution {
    public int findLengthOfLCIS(int[] nums) {
        int m=1;
        int c=1;
        int l=nums.length;
        for(int i=1;i<l;i++){
            if(nums[i]>nums[i-1]){
                c++;
            }
            else{
                m=m>c?m:c;
                c=1;
            }
        }
        m=m>c?m:c;
        return m;
    }
}