class Solution {
    public int mostWordsFound(String[] sentences) {
        int l=sentences.length;
        int m=Integer.MIN_VALUE;
        for(int i=0;i<l;i++){
            if(sentences[i].split(" ").length>m){
                m=sentences[i].split(" ").length;
            }
        }
        return m;
    }
}
