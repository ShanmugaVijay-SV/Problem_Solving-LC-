/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public int find(TreeNode rt,Queue<TreeNode> q){
        int var=rt.val;
        while(!q.isEmpty()){
            int l=q.size();
            var=q.peek().val;
            for(int i=0;i<l;i++){
                TreeNode n=q.poll();
                if(n.left!=null)
                q.offer(n.left);
                if(n.right!=null)
                q.offer(n.right);
            }
        }
        return var;
    }
    public int findBottomLeftValue(TreeNode root) {
        Queue<TreeNode> q=new LinkedList<>();
        q.offer(root);
        return find(root,q);
    }
}