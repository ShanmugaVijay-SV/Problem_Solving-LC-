class Solution {
    public int numEquivDominoPairs(int[][] domi) {
        int l=domi.length;
        int res=0;
        
        int arr[]=new int[100];
        for(int i=0;i<l;i++){
            int ky=domi[i][0]<domi[i][1] ? (domi[i][1]*10)+domi[i][0] : (domi[i][0]*10)+domi[i][1];
            res=res+arr[ky];
            arr[ky]++;
        }
        return res;
    //     int l=dominoes.length;
    //     int c=0;
    //     for(int i=0;i<l;i++){
    //         Arrays.sort(dominoes[i]);
    //     }
    //     for(int i=0;i<l;i++){
    //         for(int j=i+1;j<l;j++){
    // if((dominoes[i][0]==dominoes[j][0]) && (dominoes[i][1]==dominoes[j][1])){
    //                 c++;
    //             }
    //         }
    //     }
    //     return c;
    }
}